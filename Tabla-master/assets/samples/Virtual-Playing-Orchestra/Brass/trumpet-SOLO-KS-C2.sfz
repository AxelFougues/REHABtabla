//==============================================================

// -------------- high vol --------------
<group>
sw_default=c2  // sustain by default
sw_lokey=c2
sw_hikey=e2
sw_last=c2
sw_label=C2 Sustain

// add a little vibrato
pitchlfo_freq=3.176
pitchlfo_depth=10
pitchlfo_fade=1.5

// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1


// fade in
xfin_lovel=54
xfin_hivel=104
ampeg_attack=0.6
ampeg_decay=0.06

ampeg_release=0.6
volume=-1

