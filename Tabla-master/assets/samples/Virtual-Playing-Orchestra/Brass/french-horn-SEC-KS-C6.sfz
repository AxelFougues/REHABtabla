<group>
sw_default=c6  // sustain by default
sw_lokey=c6
sw_hikey=e6
sw_last=c6
sw_label=C6 Sustain

ampeg_attack=0.6
ampeg_release=0.6
// fade in
xfin_lovel=54
xfin_hivel=104
// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1


