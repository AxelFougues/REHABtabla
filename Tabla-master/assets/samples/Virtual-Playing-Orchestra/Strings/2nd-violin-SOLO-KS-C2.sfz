<group>
sw_default=c2  // sustain by default
sw_lokey=c2
sw_hikey=f2
sw_last=c2
sw_label=C2 Sustain

offset=300 // make 2nd violin a little different from 1st violin (PB)
eq1_freq=295.7
eq1_bw=2
eq1_gain=4.6

eq1_freq=4871.7
eq1_bw=2
eq1_gain=-4

// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1


// swap the samples around to further differentiate from 1st-SOLO violin

ampeg_attack=0.3
ampeg_release=0.6
ampeg_decay=0.6
tune=-2
width=0 // force mono (PB)

// randomize like a real player(PB)
offset_random=200
