<group>
sw_default=c2  // sustain by default
sw_lokey=c2
sw_hikey=f2
sw_last=d#2
sw_label=D#2 Accent

delay=0.09 // allow time for accent before sustain takes over
offset=300 // make 2nd violin a little different from 1st violin (PB)
eq1_freq=295.7
