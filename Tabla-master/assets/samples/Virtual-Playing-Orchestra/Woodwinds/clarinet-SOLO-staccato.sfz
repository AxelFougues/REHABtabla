// removed p samples - not worth it (PB)

<group>
// create staccato from sustain (PB)
ampeg_attack=0.03
ampeg_sustain=0
ampeg_hold=0.15
ampeg_decay=0.6
ampeg_release=0.6

width=0

ampeg_release=0.6 // slightly longer release (PB)

// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1

 group=1  pitchlfo_delay=.8 pitchlfo_fade=.3 pitchlfo_depth=-10 pitchlfo_freq=5



<region> 
sample=..\libs\NoBudgetOrch\SoloClarinet\Sustain\3_Eb_mf.wav

