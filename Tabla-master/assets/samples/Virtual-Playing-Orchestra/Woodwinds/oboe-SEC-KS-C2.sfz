//==============================================================
//  Oboe Section - Staccato
//
//   Woodwinds - Oboes Staccato.sfz from Sonatina Symphonic Orchestra - http://sso.mattiaswestlund.net
//
//   modified by Paul Battersby - http://virtualplaying.com
//
//==============================================================

// ------------------------------
//  Sonatina Symphonic Orchestra
// ------------------------------
//        Oboes Staccato
// ------------------------------

<group>
sw_default=c2  // sustain by default
sw_lokey=c2
sw_hikey=e2
sw_last=d2
sw_label=D2 Normal Mod Wheel

ampeg_attackcc1=0.5 // mod wheel slows attack
gain_cc1=-15        // mod wheel reduces volume
// manufacture staccato from sustain (PB)
offset=1530

ampeg_attack=0.04
ampeg_sustain=0
ampeg_hold=0.09
ampeg_decay=0.5
ampeg_release=0.5

// randomize like a real player(PB)
offset_random=200
pitch_random=5
amp_random=1






eq1_freq=589.4 // boost low end (PB)
eq1_gain=7.5
eq1_bw=2

eq2_freq=1237.2 // boost low end (PB)
eq2_gain=6.8
